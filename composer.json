{
    "name": "tajik-digital",
    "description": "A Laravel Nova range input filter.",
    "keywords": [
        "laravel",
        "nova",
        "range",
        "input",
        "filter",
        "tajik"
    ],
    "authors": [
        {
            "name": "Dzheka"
        },
        {
            "name": "bigmiaor"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCreative\\RangeInputFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCreative\\RangeInputFilter\\RangeInputFilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
